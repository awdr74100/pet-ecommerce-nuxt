{
  "version": 2,
  "builds": [
    {
      "src": "/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}
